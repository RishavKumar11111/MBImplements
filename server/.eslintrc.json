{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "max-len": 0,
    "quotes": 0,
    "no-console": 0,
    "eol-last": [
      "error",
      "never"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "no-unused-vars": 0,
    "no-async-promise-executor": 0,
    "no-plusplus": 0,
    "comma-dangle": [
      "error",
      "never"
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": "always",
        "ObjectPattern": {
          "multiline": true
        },
        "ImportDeclaration": "never",
        "ExportDeclaration": {
          "multiline": true
        }
      }
    ],
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "lines-between-class-members": [
      "error",
      "always",
      {
        "exceptAfterSingleLine": true
      }
    ],
    "no-undef": 0,
    "no-nested-ternary": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-useless-constructor": 0,
    "no-empty-function": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-empty": 0,
    "global-require": 0
  }
}